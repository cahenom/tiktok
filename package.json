{
  "name": "tiktok-downloader-vercel",
  "version": "1.0.0",
  "description": "TikTok video downloader deployed on Vercel",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"ts-node src/index.ts\" \"tsc --watch --project tsconfig.client.json\"",
    "start": "node dist/index.js",
    "build": "npm run build:server && npm run build:client && npm run build:css && npm run copy-static",
    "build:server": "tsc",
    "build:client": "tsc --project tsconfig.client.json",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "copy-static": "node copy-static.js",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.8.2",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "tiktok",
    "downloader",
    "vercel",
    "serverless"
  ],
  "author": "",
  "license": "ISC"
}
