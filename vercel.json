{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/output.css",
      "dest": "/output.css"
    },
    {
      "src": "/client.js",
      "dest": "/client.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
