{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/download",
      "dest": "src/index.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/public/$1"
    }
  ]
}
